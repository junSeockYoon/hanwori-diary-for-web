<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="auth">

    <select id="userType">
        SELECT 
            user_type 
        FROM users 
        WHERE USER_ID = #{id} and PW = #{password}
    </select>
     
</mapper>